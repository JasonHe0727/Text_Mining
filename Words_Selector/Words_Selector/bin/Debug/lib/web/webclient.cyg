webclient_functions = CSharpDll("CygniLibraries.dll","CygniLibraries.Web.webclient")

class WebClient {
	def __INIT__(){
		this.self_wc = webclient_functions.webclient_init()
	}
	def downloadStr(url,encoding){
		webclient_functions.webclient_downloadstr(this.self_wc,url,encoding)
	}
	def openRead(url){
		webclient_functions.webclient_openRead(this.self_wc,url)
	}
	def close(){
		dispose(this.self_wc)
	}
}
